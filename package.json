{
  "name": "quoro",
  "version": "0.1.0",
  "description": "Quoro is a website like Quora. Both frontend and backend are written in JavaScript.",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --w",
    "server": "nodemon --exec babel-node --presets es2015,react src/index.js",
    "api": "nodemon --exec babel-node --presets es2015 api/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primosdace/Quoro.git"
  },
  "keywords": [
    "quoro",
    "quora",
    "javascript",
    "fullstack"
  ],
  "author": "PRIMO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/primosdace/Quoro/issues"
  },
  "homepage": "https://github.com/primosdace/Quoro#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "history": "^4.6.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.3.0",
    "pg": "^6.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "url-search-params": "^0.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ]
  }
}
